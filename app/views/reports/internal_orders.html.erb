




<div style="width:800px;height:500px;background-color:white;margin:auto;border:3px solid #333333;overflow:scroll;">
<table class="table table-striped table-bordered">
  <tr >
    <th style="background-color:rgb(0, 108, 204);color: white;">Counter NAME </th>

    <th style="background-color:rgb(0, 108, 204);color: white;">Item </th>
     <th style="background-color:rgb(0, 108, 204);color: white;">Quantity</th>
     <th style="background-color:rgb(0, 108, 204);color: white;"></th>



  </tr>

<%@internal_orders.each do |a|%>

  <tr>
    <td><%=a.counter_name%></td>
    <td><%=a.item_name%></td>
    <td><%=a.quantity%></td>
    <td><button type="button" class="btn-mini btn-danger" onclick="cancel1(<%=a.id%>)">delivered</button></td>
  </tr>

<%end%>
</table>
    

</div>

